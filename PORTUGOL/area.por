programa {
  funcao inicio() {
    real l, h, area, qtd
    

    escreva(" digite a largura da parede em metros: ")
    leia(l)

    escreva(" digite a altura da parede em metros: ")
    leia(h)
    area = l*h
    qtd = area/2

    escreva(" a area da parede é: ", area , "\n a quantidade de tinta necessaria é: ", qtd )

    

  }
}
